<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<!--    class切换老师-->
<bean name="teacher" class="scecond.ArtTeacher"/>
<!--    autowire="byName" 自动装配-->
    <bean name="student" class="first.Student"  autowire="byName" init-method="init" destroy-method="destroy">
<!--        <property name="teacher" ref="teacher"/>-->
        <property name="name" value="卢本伟"/>
        <property name="list">
            <list>
                <value>AAA</value>
                <value>BBB</value>
                <value>CCC</value>
            </list>
        </property>
    </bean>
<bean name="artStudent" class="first.ArtStudent">
    <property name="name" value="小明"/>
    <property name="id" value="1"/>
</bean>

<alias name="artStudent" alias="artStudent"/>
<!--    继承artStudent中的属性，重写id-->
<bean name="sportStudent" class="first.SportStudent" parent="artStudent" >
    <property name="id" value="2"/>
</bean>
</beans>